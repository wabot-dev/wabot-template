{
  "name": "@wabot-dev/template",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "yts --import=./env.mjs --watch --watch-path=src --watch-preserve-output ./src/run.ts",
    "build": "tsup src/run.ts --format esm --target es2022 --platform node --out-dir dist --clean --sourcemap",
    "start:prod": "node --import ./env.mjs dist/run.mjs",
    "fmt": "prettier --write .",
    "tsc": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@wabot-dev/framework": "^0.0.50"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "tsup": "^8.5.0"
  }
}
