{
  "name": "@wabot-dev/template",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
 "scripts": {
    "dev": "node --import=./env.mjs --import @yucacodes/ts/src/custom-import.mjs  ./src/run.ts",
    "test": "node --import @yucacodes/ts/src/custom-import.mjs --test .//*.test.ts",
    "fmt": "prettier --write .",
    "fmt:check": "prettier --check .",
    "build": "tsup-node ./src/run.ts --format esm --clean --no-splitting --sourcemap",
    "start": "node ./dist/run.js"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@wabot-dev/framework": "0.1.0-beta.73"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "tsup": "^8.5.0"
  }
}
